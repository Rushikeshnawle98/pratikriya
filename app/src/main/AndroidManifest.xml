		android:icon="@drawable/feedback_app_icon"
		android:label="@string/app_name"
		android:roundIcon="@drawable/feedback_app_icon"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">
		<activity android:name=".GetFeedbackActivity"></activity>
		<activity android:name=".AdminDisplay" />
